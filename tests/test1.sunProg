program {
	int a, count = 0, p = 1, s = 0;
	# string s = "oooooy", s1, s2;

	write("enter a: ");
	read(a);

	while (a > 0) {
		p *= a % 10;
		s = s + a % 10;
		count++;

		a /= 10;
	}

	writeln("p: ", p);
	writeln("s: ", s);
	writeln("count: ", count);

	#write("a = ", a, "i = " , i, "s = ", s, "5 * (7 - 8) = ", 5 * (7 - 8));
}